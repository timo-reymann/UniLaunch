<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="MenuBar.Tab.Background" StartPoint="100%,0%" EndPoint="100%,100%">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0" Color="#27213C"/>
            <GradientStop Offset="1"  Color="#3A2F5B"/>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="MenuBar.Background" Color="#27213C"/>
    <SolidColorBrush x:Key="MenuBar.Background.Accent" Color="#3A2F5B"/>
    <SolidColorBrush x:Key="MenuBar.PrimaryMenuItem.Foreground" Color="#E2E2E2" />
    <SolidColorBrush x:Key="SidePanel.Background" Color="#14FFFFFF" />
</ResourceDictionary>